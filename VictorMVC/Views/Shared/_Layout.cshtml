<!-- Website designed by Josh Spence http://joshspence.info-->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta property="og:title" content="Victor Aguilar: Voice Actor" />
    <meta property="og:url" content="https://victoraguilarvoices.com" />
    <meta property="og:type" content="website" />
    <meta property="og:image" content="https://victoraguilarvoices.com/Content/Images/og-img.jpg" />
    <meta property="og:image:width" content="1200" />
    <meta property="og:image:height" content="600" />
    <meta property="og:description" content="Victor Aguilar is a Southern California based voice actor that is capable of 
          fulfilling a variety of casting needs. From commercials to animation and ADR, he has been trained by well respected 
          instructors in the voice over industry who gave him the skill set to perform whatever your projects needs." />
    <title>@ViewBag.Title</title>
    <link rel="icon" type="image/x-icon" href="favicon.ico">
    @Styles.Render("~/content/bundles/css")
    @RenderSection("Styles", required: false)
</head>
<body class="text-white">
    <nav class="navbar navbar-expand-md navbar-dark bg-secondary shadow fixed-top my-md-0 py-md-0">
        <a class="navbar-brand" href="/#">Victor Aguilar</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse justify-content-end mr-md-5 my-md-0" id="navbarSupportedContent">
            <ul class="navbar-nav">
                <li class="nav-item p-md-2">
                    <a class="nav-link active" href="/#about">About</a>
                </li>
                <li class="nav-item p-md-2">
                    <a class="nav-link active" href="/#demos">Demos</a>
                </li>
                <li class="nav-item p-md-2">
                    <a class="nav-link active" href="/#contact">Contact</a>
                </li>
            </ul>
        </div>
    </nav>
    <div class="nav-padding"></div>
    @RenderSection("Jumbotron", required: false)
    @RenderBody()
    <hr />
    <footer class=" container">
        <div class="row">
            <p class="col-6 text-white">&copy; @DateTime.Now.Year - Victor Aguilar</p>
            <p class="col-6 text-right text-white">Powered by <a href="https://joshspence.info" target="_blank" class="text-dark">Josh Spence</a></p>
        </div>
        <!-- additional padding for tablets or bigger so contact header appears at top of screen when linked to -->
        <div class="py-5 d-none d-md-block"></div>
    </footer>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("Scripts", required: false)
</body>
</html>